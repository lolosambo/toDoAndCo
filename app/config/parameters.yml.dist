    MYSQL_PASSWORD: yourPassword ### It will be define in your docker environment too
    MYSQL_PORT: 3306
    DB_HOST: mysql
    DB_DRIVER_DEV: pdo_mysql
    DB_CHARSET_DEV: utf8
    DATABASE_URL_DEV: mysql://yourUsername:yourPassword@mysql/toDoAndCo
    REDIS_URL: redis://localhost