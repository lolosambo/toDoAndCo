parameters:

    mailer_transport:  smtp
    mailer_host:       yourIpAddress
    mailer_user:       ~
    mailer_password:   ~

    WORKPATH: /var/www/toDoAndCo
    COMPOSER_ALLOW_SUPERUSER: 1
    APP_SECRET: 72e4c82ed7088d4da4209cf01708a279
    CONTAINER_NAME: toDoAndCo
    URL_ROOT: http://yourIpAddress:8086/
    MYSQL_CHARSET: utf8
    MYSQL_DB: toDoAndCo
    MYSQL_ROOT_PASSWORD: root
    MYSQL_DATABASE: toDoAndCo
